{
  "name": "senna",
  "version": "2.7.8",
  "description": "A blazing-fast Single Page Application engine",
  "license": "BSD-3-Clause",
  "repository": "liferay/senna.js",
  "engines": {
    "node": ">=0.12.0",
    "npm": ">=3.0.0"
  },
  "jsnext:main": "src/senna.js",
  "main": "lib/senna.js",
  "files": [
    "build",
    "lib",
    "src/**/*.js",
    "test/**/*.js"
  ],
  "scripts": {
    "build": "gulp build build:amd",
    "compile": "BABEL_ENV=compile babel -d lib/ src/",
    "checkFormat": "prettier --check '{src,test}/**/*.js'",
    "format": "prettier --write '{src,test}/**/*.js'",
    "lint": "eslint {src,test}/ --config eslint.config.js",
    "prepublish": "gulp && npm run compile",
    "test": "gulp test",
    "test:debug": "karma start --debug",
    "test:watch": "gulp test:watch",
    "test:saucelabs": "gulp test:saucelabs"
  },
  "keywords": [
    "spa",
    "single",
    "page",
    "apps"
  ],
  "dependencies": {
    "bootstrap": "^3.3.6",
    "metal": "^2.16.5",
    "metal-dom": "^2.16.8",
    "metal-events": "^2.16.5",
    "metal-path-parser": "^1.0.3",
    "metal-uri": "^2.2.6"
  },
  "devDependencies": {
    "babel-cli": "^6.4.5",
    "babel-eslint": "^10.0.2",
    "babel-plugin-search-and-replace": "0.3.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "babel-preset-metal": "^4.0.0",
    "chai": "^4.2.0",
    "es6-object-assign": "^1.1.0",
    "eslint": "^5.16.0",
    "eslint-config-liferay": "^3.0.0",
    "gulp": "^3.8.11",
    "gulp-connect": "^5.0.0",
    "gulp-header": "^1.7.1",
    "gulp-jsdoc3": "^1.0.1",
    "gulp-metal": "^1.0.0",
    "gulp-rename": "^1.2.2",
    "gulp-strip-debug": "^1.1.0",
    "gulp-template": "^4.0.0",
    "karma": "^4.1.0",
    "metal-karma-config": "^2.3.1",
    "metal-tools-build-rollup": "^2.0.6",
    "mocha": "^6.2.0",
    "prettier": "^1.18.2",
    "promise-polyfill": "^8.1.3",
    "run-sequence": "^1.1.5",
    "sinon": "^5.0.0",
    "whatwg-fetch": "^3.0.0"
  }
}
