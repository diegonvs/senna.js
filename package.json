    "gulp-rename": "^1.2.2",
    "gulp-strip-debug": "^1.1.0",
    "metal-tools-build-rollup": "^2.0.6",
    "progress-bar-webpack-plugin": "^1.10.0",
    "run-sequence": "^1.1.5",
    "sinon": "1.17.7",
    "uglifyjs-webpack-plugin": "^1.1.2",
    "webpack": "^3.10.0",
    "webpack-dashboard": "^1.0.2"
