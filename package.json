    "metal-dom": "^2.0.0",
    "metal-events": "^2.0.0",
    "metal-structs": "^1.0.0",
    "metal-path-parser": "^1.0.0",
    "metal-promise": "^2.0.0",
    "metal-uri": "^2.2.6",
    "metal-useragent": "^2.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.34",
    "@babel/preset-env": "^7.0.0-beta.34",
    "babel-cli": "^6.4.5",
    "babel-loader": "^8.0.0-beta.0",
    "babel-preset-metal": "^4.0.0",
    "compression-webpack-plugin": "^1.0.1",
    "gulp": "^3.8.11",
    "gulp-connect": "^5.0.0",
    "gulp-header": "^1.7.1",
